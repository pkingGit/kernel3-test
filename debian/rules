#!/usr/bin/make -f

srctree ?= .

build-indep:
build-arch:
	$(MAKE) KERNELRELEASE=5.15.67-test-project3-kernel3+ ARCH=x86 	KBUILD_BUILD_VERSION=14 -f $(srctree)/Makefile

build: build-arch

binary-indep:
binary-arch: build-arch
	$(MAKE) KERNELRELEASE=5.15.67-test-project3-kernel3+ ARCH=x86 	KBUILD_BUILD_VERSION=14 -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
